#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main(void) {
    char buffer[100];

    if (fgets(buffer, sizeof(buffer), stdin) == NULL) {
        printf("Nie jest palindromem\n");
        return 0;
    }

    size_t len = strlen(buffer);
    if (len > 0 && buffer[len - 1] == '\n') {
        buffer[len - 1] = '\0';
        len--;
    }

    size_t left = 0;
    size_t right = len - 1;

    while (left < right) {
        char a = tolower(buffer[left]);
        char b = tolower(buffer[right]);

        if (a != b) {
            printf("Nie jest palindromem\n");
            return 0;
        }

        left++;
        right--;
    }

    printf("Jest palindromem\n");
    return 0;
}
