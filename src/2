#include <stdio.h>

int main(void) {
    int R, C;

    // Wczytanie wymiarów
    if (scanf("%d %d", &R, &C) != 2 || R < 1 || C < 1) {
        printf("BŁĄD DANYCH WEJŚCIOWYCH\n");
        return 0;
    }

    // Deklaracja VLA
    int matrix[R][C];

    // Wczytanie R×C elementów
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            if (scanf("%d", &matrix[i][j]) != 1) {
                printf("BŁĄD DANYCH WEJŚCIOWYCH\n");
                return 0;
            }
        }
    }

    // Wypisanie transpozycji C×R
    for (int i = 0; i < C; i++) {
        for (int j = 0; j < R; j++) {
            printf("%d", matrix[j][i]);
            if (j < R - 1) printf(" ");
        }
        printf("\n");
    }

    return 0;
}
